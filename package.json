{
  "name": "semantic-release-tamia",
  "description": "Custom scripts for semantic-release",
  "version": "0.0.0-development",
  "scripts": {
    "test": "true && npm run test:jest",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write '**/*.{js,md}'",
    "posttest": "npm run format"
  },
  "author": "Artem Sapegin <artem@sapegin.ru> (http://sapegin.me/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamiadev/semantic-release-tamia.git"
  },
  "dependencies": {
    "conventional-commits-parser": "^3.0.0",
    "indent": "0.0.2"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "prettier": "^1.14.2"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "lib",
    "analyzeCommits.js",
    "generateNotes.js",
    "verifyRelease.js"
  ]
}
